<?xml version="1.0" encoding="utf-8"?> 
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN" 
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>    
    <class name="modelo.pojo.Token"
           table="tokenAcceso">
        <id name="id" column="id">
            <generator class="native"/>
        </id>

        <property name="idPublico" column="id_publico" type="string"/>

        <!-- Usuario al que esta ligado el token -->
        <many-to-one name="usuario"
            column="id_usuario"
            class="modelo.pojo.Usuario"
            not-null="true"/>
    </class>
</hibernate-mapping>

